        </activity>
        <activity
            android:name=".Home"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme" />
        <activity android:name=".PaperWebView" />
        <activity android:name=".PaperResult" />
        <activity android:name=".PurchasePaper" />
